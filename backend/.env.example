# Feature Flags
PHOENIX_ENABLE_BLOG=true
PHOENIX_ENABLE_USER_AI_CREATION=true
PHOENIX_ENABLE_PUBLIC_AI_GALLERY=true
PHOENIX_ENABLE_AI_RATINGS=true
PHOENIX_ENABLE_FAVORITES=true
PHOENIX_ENABLE_USER_PROFILES=true
PHOENIX_ENABLE_REFERRAL_SYSTEM=false
