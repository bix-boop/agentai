PHOENIX_MAINTENANCE_ALLOWED_IPS=

# Logging
PHOENIX_LOG_API_REQUESTS=false
PHOENIX_LOG_CHAT_MESSAGES=true
PHOENIX_LOG_PAYMENTS=true
PHOENIX_LOG_USER_ACTIONS=false

# Performance
PHOENIX_ENABLE_QUERY_CACHING=true
PHOENIX_ENABLE_RESPONSE_COMPRESSION=true
PHOENIX_ENABLE_CDN=false
PHOENIX_CDN_URL=

# Security
PHOENIX_ENABLE_CSRF_PROTECTION=true
PHOENIX_ENABLE_CORS=true
