# OpenAI Configuration
OPENAI_API_KEY=
OPENAI_ORGANIZATION=

# Phoenix AI Configuration
PHOENIX_APP_NAME="${APP_NAME}"
PHOENIX_APP_DESCRIPTION="Advanced AI Assistant Platform"
PHOENIX_ALLOW_REGISTRATION=true
PHOENIX_REQUIRE_EMAIL_VERIFICATION=true
PHOENIX_WELCOME_CREDITS=1000
PHOENIX_MAX_FAILED_LOGIN_ATTEMPTS=5
PHOENIX_LOGIN_LOCKOUT_MINUTES=30

# Credit System
PHOENIX_CREDIT_PER_CHARACTER=1
PHOENIX_CREDIT_PER_TOKEN=4
PHOENIX_IMAGE_CREDITS_256=1000
PHOENIX_IMAGE_CREDITS_512=2000
PHOENIX_IMAGE_CREDITS_1024=4000
PHOENIX_IMAGE_CREDITS_1792_1024=6000
PHOENIX_IMAGE_CREDITS_1024_1792=6000

# AI Configuration
PHOENIX_DEFAULT_AI_MODEL=gpt-3.5-turbo
PHOENIX_MAX_TOKENS=2000
PHOENIX_DEFAULT_TEMPERATURE=0.7
PHOENIX_DEFAULT_FREQUENCY_PENALTY=0.0
PHOENIX_DEFAULT_PRESENCE_PENALTY=0.0
PHOENIX_OPENAI_TIMEOUT=120

# Chat Configuration
PHOENIX_MAX_MESSAGE_LENGTH=5000
PHOENIX_MIN_MESSAGE_LENGTH=1
PHOENIX_CONVERSATION_MEMORY=10
PHOENIX_MAX_FREE_MESSAGES=5
PHOENIX_ENABLE_MESSAGE_STREAMING=true
PHOENIX_ENABLE_VOICE_FEATURES=true
PHOENIX_ENABLE_IMAGE_GENERATION=true

# Content Safety
PHOENIX_ENABLE_CONTENT_FILTER=true
PHOENIX_AUTO_FLAG_THRESHOLD=0.8
PHOENIX_ENABLE_OPENAI_MODERATION=true

# Payment Configuration
PHOENIX_CURRENCY=USD
PHOENIX_STRIPE_ENABLED=true
PHOENIX_PAYPAL_ENABLED=true
PHOENIX_BANK_DEPOSIT_ENABLED=true

# Stripe
STRIPE_KEY=
STRIPE_SECRET=
STRIPE_WEBHOOK_SECRET=

# PayPal
PAYPAL_CLIENT_ID=
PAYPAL_CLIENT_SECRET=
PAYPAL_WEBHOOK_ID=
PAYPAL_MODE=sandbox

# File Upload Configuration
PHOENIX_MAX_AVATAR_SIZE=2048
PHOENIX_MAX_AI_AVATAR_SIZE=2048
PHOENIX_MAX_PACKAGE_IMAGE_SIZE=1024
PHOENIX_AVATAR_WIDTH=640
PHOENIX_AVATAR_HEIGHT=640

# Email Configuration
PHOENIX_MAIL_FROM_NAME="${APP_NAME}"
PHOENIX_MAIL_FROM_ADDRESS="noreply@your-domain.com"
PHOENIX_SUPPORT_EMAIL="support@your-domain.com"

# Rate Limiting
PHOENIX_API_RATE_LIMIT=60
PHOENIX_CHAT_RATE_LIMIT=20
PHOENIX_IMAGE_RATE_LIMIT=10
PHOENIX_REGISTRATION_RATE_LIMIT=5

# Cache Configuration
PHOENIX_CACHE_AI_ASSISTANTS_TTL=3600
PHOENIX_CACHE_CATEGORIES_TTL=7200
PHOENIX_CACHE_SETTINGS_TTL=1800
PHOENIX_CACHE_USER_CREDITS_TTL=300

# UI/UX Configuration
PHOENIX_ENABLE_DARK_MODE=true
PHOENIX_DEFAULT_THEME=light
PHOENIX_ENABLE_ANIMATIONS=true
PHOENIX_SHOW_CREDIT_BALANCE=true
PHOENIX_SHOW_PROCESSING_TIME=false

# SEO Configuration
PHOENIX_META_TITLE="Phoenix AI - Advanced AI Assistant Platform"
PHOENIX_META_DESCRIPTION="Experience the power of AI with Phoenix AI. Chat with intelligent assistants, generate images, and unlock premium features."
PHOENIX_META_KEYWORDS="AI, artificial intelligence, chatbot, assistant, OpenAI, GPT"
PHOENIX_ENABLE_SITEMAP=true
PHOENIX_ENABLE_ROBOTS_TXT=true

# Analytics
GOOGLE_ANALYTICS_ID=
FACEBOOK_PIXEL_ID=
PHOENIX_ENABLE_INTERNAL_ANALYTICS=true
PHOENIX_TRACK_USER_BEHAVIOR=true

